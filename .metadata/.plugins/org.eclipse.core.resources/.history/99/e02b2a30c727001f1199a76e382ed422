package chap04;

public class ForPrintExam {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		for(int i=0 , j=100; i<=10 && j >=90; i++, j--) {
		System.out.println("i값의 증가 : " + i + " // j값의 감소 : " + j);
	
	}
		
		System.out.println("=====================================");
		
		for(int i = 1 ; i <=10 ; i++) {
			System.out.println(" 1 ~ 10까지의 출력 : " + i);
		}
	}
}
